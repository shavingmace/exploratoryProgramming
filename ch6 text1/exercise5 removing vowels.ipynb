    "def devowel(string):\n",
    "    arr = []\n",
    "    for elem in list(string):\n",
    "        if elem.lower() not in ['a', 'e', 'i', 'o', 'u']:\n",
    "            arr.append(elem)\n",
    "    return ''.join(arr)"
   ]
